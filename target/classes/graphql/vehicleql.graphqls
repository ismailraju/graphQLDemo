type Vehicle {
    id: ID!,
    type: String,
    modelCode: String,
    brandName: String,
    launchDate: String
}

type Query {
    vehicles(count: Int):[Vehicle]
    vehicle(id: ID):Vehicle
}
type Mutation {
    createVehicle(type: String!, modelCode: String!, brandName: String, launchDate: String):Vehicle

}


type Food{
    id:ID!
    name:String
    price:Float
}

type Query{
    foods:[Food]
    food(id:ID):Food
}

type Mutation {
    createFood(id:ID!,name:String,price:Float):Food
}